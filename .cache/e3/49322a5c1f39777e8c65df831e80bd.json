ttribution {\n  font-size: 11px;\n  text-align: center;\n  a {\n    color: hsl(228, 45%, 44%);\n  }\n}\n\nul {\n  list-style: none;\n}\n\na {\n  text-decoration: none;\n}\n\nbutton {\n  background: transparent;\n  border: none;\n  cursor: pointer;\n  outline: none;\n  color: $white-color;\n}\n\n.header {\n  background: url(./images/bg-pattern-intro.svg),\n    linear-gradient(135deg, $light-red-color 0%, $red-color 100%);\n  background-position: 24% 52%;\n  border-radius: 0px 0px 0px 100px;\n  padding: 63px 161px 155px 167px;\n  &__nav {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    font-family: $ubuntu-font;\n  }\n  &__content-link {\n    display: flex;\n    align-items: center;\n  }\n  &__list {\n    display: flex;\n    margin-left: 64px;\n  }\n  &__item {\n    margin-right: 32px;\n    line-height: 18px;\n    font-weight: bold;\n  }\n  &__link--white {\n    color: $white-color;\n    opacity: 0.75;\n  }\n  &__dropdown {\n    display: none;\n    border-radius: 5px;\n    background: #ffffff;\n    box-shadow: 0px 20px 40px rgba(0, 0, 0, 0.243444);\n    padding: 24px;\n    position: relative;\n    top: 27px;\n    left: -22px;\n    .dropdown__item {\n      font-weight: normal;\n      font-size: 15px;\n    }\n  }\n\n  .nav__btns {\n    &--login {\n      font-family: $ubuntu-font;\n      opacity: 0.75;\n      color: $white-color;\n    }\n    &--white {\n      @include button($white-color, $text-red-color);\n      opacity: 1;\n      padding: 15px 39px;\n      margin-left: 32px;\n      &:hover {\n        @include button($lighter-red-color, $white-color);\n        transition: 0.5s ease;\n      }\n    }\n  }\n\n  &__main {\n    text-align: center;\n    font-family: $overpass-font;\n    color: $white-color;\n    margin-top: 119px;\n    &--title {\n      font-size: 64px;\n      font-weight: 600;\n      line-height: 98px;\n    }\n    &--subtitle {\n      font-weight: 300;\n      font-size: 20px;\n      line-height: 31px;\n      margin-bottom: 43px;\n    }\n  }\n\n  &__btn--start {\n    @include button($white-color, $text-red-color);\n    padding: 15px 17px;\n    margin-right: 16px;\n    &:hover {\n      @include button($lighter-red-color, $white-color);\n      transition: 0.5s ease;\n    }\n  }\n  &__btn--learn {\n    @include button(inherit, inherit);\n    border: 1px solid $white-color;\n    padding: 15px 25px;\n    border-radius: 28px;\n    &:hover {\n      @include button($white-color, $lighter-red-color);\n      transition: 0.5s ease;\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"6b23829ef39014028cfeae07eb202379","cacheData":{"env":{}}}